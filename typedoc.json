{
  "entryPoints": ["src"],
  "out": "./docs/dist",
  "customCss": "docs/typedoc.css",
  "cleanOutputDir": true,
  "plugin": ["typedoc-material-theme"],
  "themeColor": "#B7FB5A",
  "categorizeByGroup": true,
  "groupOrder": ["Functions", "*"],
  "categoryOrder": ["Validators", "*"],
  "navigation": {
    "includeGroups": true,
    "includeCategories": true
  }
}
